{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "bulma": "^0.4.1",
    "jest-enzyme": "^3.1.0",
    "open-color": "^1.5.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.8.2",
    "node-sass": "^4.5.2",
    "react-test-renderer": "^15.5.4"
  },
  "dependencies": {
    "d3": "^4.9.1",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1"
  },
  "scripts": {
    "build-css": "node_modules/node-sass/bin/node-sass src/ -o src/",
    "watch-css": "npm run build-css && node_modules/node-sass/bin/node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm run start-js && watch-css",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts/bin/react-scripts eject"
  }
}
